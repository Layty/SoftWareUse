///
/// @brief显示ASCII 表格
///
macro HelpShowAsciiTable()
{
    var path;
    var hFile;

	path = __common_get_help_path("ascii_table.h");
	hFile = OpenBuf(path);


    if(hFile == hNil)
    {
    	stop;
    }

   	SetCurrentBuf(hFile);
}
